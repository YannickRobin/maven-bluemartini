    "CompileAttributes" DNA {
        "class" String "com.bluemartini.bizact.CompileAttributes",
        "database" String "object",
        "autoTransaction" Boolean "false",
        "input" DNAArray [
            {
                "path" StringArray [ "version" ],
                "required" Boolean "true",
                "type" String "Integer"
            },{
                "path" StringArray [ "objectId" ],
                "type" String "Long"
            }
        ],

        // number of threads to use during bulk compilation
        "threads" Integer "5",

        // query to identify what objects require compilation
        "bulkSql" StringArray [
             "SELECT OBJ_ID FROM OBJECT WHERE (OBJ_ATTR_UPTODATE IS NULL OR OBJ_ATTR_UPTODATE!='T') AND VERSION={version}",
             "SELECT CNT_ID FROM CONTENT WHERE (CNT_ATTR_UPTODATE IS NULL OR CNT_ATTR_UPTODATE!='T') AND VERSION={version}",
             "SELECT PRD_ID FROM PRODUCT WHERE (PRD_ATTR_UPTODATE IS NULL OR PRD_ATTR_UPTODATE!='T') AND VERSION={version}",
             "SELECT SKU_ID FROM SKU WHERE (SKU_ATTR_UPTODATE IS NULL OR SKU_ATTR_UPTODATE!='T') AND VERSION={version}",
             "SELECT REL_ID FROM RELATIONSHIP WHERE (REL_ATTR_UPTODATE IS NULL OR REL_ATTR_UPTODATE!='T') AND VERSION={version}"
        ],

        // Number of seconds to overlap incremental updates. The time
        // fields have second resolution, so there could be rounding issues
        // if we do not deliberately overlap.
        "incrOverlapSecs" Integer "1"

        // Incremental attribute update. Comment this out to peform
        // non-incremental compiles.

        // Alternate incremental update based upon changes to AUDIT_LOG. This
        // benefits from a range scan on the index on the audit table, but may
        // miss objects. A miss should not be serious as any missed object
        // will be compiled when it is first accessed.
        // "incrSql" StringArray [
        //     "SELECT DISTINCT AUD_OBJ_ID FROM AUDIT_LOG WHERE AUD_CREATE_DT >= {lastCompile} AND AUD_VERSION={version} AND AUD_TABLE='OBJECT_ATTRIBUTE'"
        // ]
    },
